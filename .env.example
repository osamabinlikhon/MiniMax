# MiniMax Agent Environment Configuration

# OpenRouter API Key (Replace with your actual key)
OPENROUTER_API_KEY=your-openrouter-api-key-here

# GitHub Token (Replace with your actual token)
GITHUB_TOKEN=your-github-token-here

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/minimax_agent
SUPABASE_URL=your-supabase-url
SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Redis Configuration
REDIS_URL=redis://localhost:6379

# Application Configuration
NODE_ENV=development
PORT=3000
API_PORT=4000
PLANNER_PORT=4001

# Authentication
JWT_SECRET=your-jwt-secret-key-here
SESSION_SECRET=your-session-secret-here

# Stripe Configuration (for payments)
STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=whsec_your-stripe-webhook-secret

# Yahoo Finance API
YAHOO_FINANCE_API_KEY=your-yahoo-finance-api-key

# Rate Limiting
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000

# File Upload Configuration
MAX_FILE_SIZE=50MB
UPLOAD_DIR=./uploads

# Logging
LOG_LEVEL=info
LOG_FILE=./logs/minimax-agent.log

# Agent Configuration
MAX_CONCURRENT_AGENTS=5
DEFAULT_AGENT_TIMEOUT=300000
PLANNING_TIMEOUT=60000

# Feature Flags
ENABLE_DARK_MODE=true
ENABLE_CUSTOM_MODE=true
ENABLE_LIGHTNING_MODE=true
ENABLE_BRANCH_SESSIONS=true
ENABLE_VERSION_CONTROL=true
ENABLE_STRIPE_INTEGRATION=true
ENABLE_SUPABASE_BACKEND=true
ENABLE_TEAM_PLAN=true
ENABLE_CUSTOM_DOMAIN=true
ENABLE_PERSISTENT_FILES=true
ENABLE_PARALLEL_PROCESSING=true
ENABLE_DIRECT_CODE_EDITING=true
ENABLE_CONVERSATIONAL_HELP=true
ENABLE_YAHOO_FINANCE=true

# CORS Configuration
CORS_ORIGIN=http://localhost:3000
CORS_CREDENTIALS=true

# WebSocket Configuration
WS_PORT=4002

# MCP Configuration
MCP_REGISTRY_URL=http://localhost:4003
MCP_TIMEOUT=30000

# Monitoring and Analytics
SENTRY_DSN=your-sentry-dsn
ANALYTICS_ENABLED=true
